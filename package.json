  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/distributions-io/generator-distributions-io.git"
  },
  "keywords": [
    "yeoman-generator",
    "distributions.io",
    "distributions",
    "probability",
    "statistics",
    "stats"
  ],
  "bugs": {
    "url": "https://github.com/distributions-io/generator-distributions-io/issues"
  },
  "dependencies": {
    "chalk": "^0.5.1",
    "mkdirp": "^0.5.1",
    "npm-name": "^1.0.1",
    "shelljs": "^0.3.0",
    "validate.io-array": "^1.0.6",
    "yeoman-generator": "^0.20.1",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "2.x.x",
    "chai": "3.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x"
  },
  "peerDependencies": {
    "yo": ">=1.0.0"
  },
