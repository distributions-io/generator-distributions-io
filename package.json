{
  "name": "generator-distributions-io",
  "version": "0.1.2",
  "description": "A Yeoman generator for Distributions.io modules.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
